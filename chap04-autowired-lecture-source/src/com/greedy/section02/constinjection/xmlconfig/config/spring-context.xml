<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">
	
	<context:component-scan base-package="com.greedy.section02.constinjection.xmlconfig"/>
	
	<bean id="randomGenerator" class="com.greedy.section02.constinjection.xmlconfig.RandomNumberGenerator">
		<constructor-arg name="startNum" value="1"/>
		<constructor-arg name="endNum" value="10"/>
	</bean>
	
	<!-- <bean id="makeRandomString" class="com.greedy.section02.constinjection.xmlconfig.MakeRandomString">
		<constructor-arg index="0" ref="randomGenerator"/>
	</bean> -->
</beans>
